{
  "ReverseProxy": {
    "Routes": {
      "auth": {
        "ClusterId": "auth-cluster",
        "Match": { "Path": "/auth/{**catch-all}" }
      },
      "users": {
        "ClusterId": "users-cluster",
        "Match": { "Path": "/users/{**catch-all}" }
      },
      "movies": {
        "ClusterId": "movies-cluster",
        "Match": { "Path": "/movies/{**catch-all}" }
      },
      "catalog": {
        "ClusterId": "catalog-cluster",
        "Match": { "Path": "/showtimes/{**catch-all}" }
      },
      "bookings": {
        "ClusterId": "bookings-cluster",
        "Match": { "Path": "/bookings/{**catch-all}" }
      },
      "rooms": {
        "ClusterId": "bookings-cluster",
        "Match": { "Path": "/rooms/{**catch-all}" }
      },
      "reviews": {
        "ClusterId": "reviews-cluster",
        "Match": { "Path": "/reviews/{**catch-all}" }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth": { "Address": "http://auth-api/" }
        }
      },
      "users-cluster": {
        "Destinations": {
          "users": { "Address": "http://users-api/" }
        }
      },
      "movies-cluster": {
        "Destinations": {
          "movies": { "Address": "http://movies-api/" }
        }
      },
      "catalog-cluster": {
        "Destinations": {
          "catalog": { "Address": "http://catalog-api/" }
        }
      },
      "bookings-cluster": {
        "Destinations": {
          "bookings": { "Address": "http://bookings-api/" }
        }
      },
      "reviews-cluster": {
        "Destinations": {
          "reviews": { "Address": "http://reviews-api/" }
        }
      }
    }
  }
}
